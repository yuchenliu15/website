<h1>Welcome</h1>

<%= form_tag({:action => "create"}, :multipart => true) do %>
    <div class="field">
        <%= label_tag :file, 'resume' %>
        <%= file_field_tag :file, accept: 'application/pdf' %>
    </div>

    <div class="submit">
        <%= submit_tag 'upload'%>
    </div>
<% end %>

